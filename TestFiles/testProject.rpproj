<?xml version="1.0"?>
<ReplacerProject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <FileFolderPaths>
    <FileFolderPath>
      <RootPath>C:\Users\sstundzia\Desktop\tests</RootPath>
      <PatternType>FileName</PatternType>
      <IncludeSvnFolders>false</IncludeSvnFolders>
    </FileFolderPath>
  </FileFolderPaths>
  <PatternList>
    <ReplacePattern>
      <Find>(?m)NewGUID</Find>
      <Replace>$&amp;==${&lt;NewGUID&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewFileGUID\r?$</Find>
      <Replace>$&amp;==${&lt;NewFileGUID&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewFileGUID\(\)\r?$</Find>
      <Replace>$&amp;==${&lt;NewFileGUID()&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewFileGUID\(a\)\r?$</Find>
      <Replace>$&amp;==${&lt;NewFileGUID(a)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewFileGUID\(b\)\r?$</Find>
      <Replace>$&amp;==${&lt;NewFileGUID(b)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewConstGUID\r?$</Find>
      <Replace>$&amp;==${&lt;NewConstGUID&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewConstGUID\(\)\r?$</Find>
      <Replace>$&amp;==${&lt;NewConstGUID()&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewConstGUID\(a\)\r?$</Find>
      <Replace>$&amp;==${&lt;NewConstGUID(a)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)NewConstGUID\(b\)\r?$</Find>
      <Replace>$&amp;==${&lt;NewConstGUID(b)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateShort</Find>
      <Replace>$&amp;==${&lt;DateShort&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateLong</Find>
      <Replace>$&amp;==${&lt;DateLong&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)TimeShort</Find>
      <Replace>$&amp;==${&lt;TimeShort&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)TimeLong</Find>
      <Replace>$&amp;==${&lt;TimeLong&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateTimeFormat\r?$</Find>
      <Replace>$&amp;==${&lt;DateTimeFormat&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateTimeFormat\(\)\r?$</Find>
      <Replace>$&amp;==${&lt;DateTimeFormat()&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateTimeFormat\(yyyy\)\r?$</Find>
      <Replace>$&amp;==${&lt;DateTimeFormat(yyyy)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateTimeFormat\(yyyy-MM\)\r?$</Find>
      <Replace>$&amp;==${&lt;DateTimeFormat(yyyy-MM)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)DateTimeFormat\(yyyy-MM-dd\)\r?$</Find>
      <Replace>$&amp;==${&lt;DateTimeFormat(yyyy-MM-dd)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)ToUpper\s(?&lt;input&gt;\w+)</Find>
      <Replace>$&amp;==${&lt;ToUpper(input)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)ToLower\s(?&lt;input&gt;\w+)</Find>
      <Replace>$&amp;==${&lt;ToLower(input)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)ToUpperChar\s(?&lt;input&gt;\w+)</Find>
      <Replace>$&amp;==${&lt;ToUpperChar(input)&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)ToUpperChar\s(?&lt;input&gt;\w+)</Find>
      <Replace>$&amp;==${&lt;ToUpperChar(input[2])&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)ToUpperChar\s(?&lt;input&gt;\w+)</Find>
      <Replace>$&amp;==${&lt;ToUpperChar(input[2,6])&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
    <ReplacePattern>
      <Find>(?m)ToUpperChar\s(?&lt;input&gt;\w+)</Find>
      <Replace>$&amp;==${&lt;ToUpperChar(input[2,6,0-1,8-10])&gt;}</Replace>
      <ReplaceType>Regex</ReplaceType>
    </ReplacePattern>
  </PatternList>
